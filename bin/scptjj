#!/bin/bash

machine=dhcp-153-55.eecs.berkeley.edu
lname=jj

if [ $# == 2 ]; then
    echo "scp $1 $lname@$machine:$2"
    scp $1 $lname@$machine:$2
    exit
fi
if [ $# == 3 ]; then
    echo "scp $1 $2 $lname@$machine:$3"
    scp $1 $2 $lname@$machine:$3
    exit
fi



echo ""
echo "usage: scptjj [-r] [local-file] [cs-file]"
echo ""
