#!/bin/bash

machine=dhcp-153-95.eecs.berkeley.edu
lname=bteam


if [ $# == 2 ]; then
    echo "scp $lname@$machine:$1 $2"
    scp $lname@$machine:$1 $2
    exit
fi
if [ $# == 3 ]; then
    echo "scp $1 $lname@$machine:$2 $3"
    scp $1 $lname@$machine:$2 $3
    exit
fi

echo ""
echo "usage: scpfjj [-r] [cs-file] [local-file]"
echo ""
